		description="COM_SPORTSMANAGEMENT_FES_PARAMS_DESCR_RSSFEED_PRO_DATA">
		
	<field name="rsstitle" type="extensionradiobutton" default="1" class="radio btn-group btn-group-yesno" label="Feed Title" description="Display news feed provider title">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>
  <field name="rsstitle_linkable" type="extensionradiobutton" default="1" class="radio btn-group btn-group-yesno" label="Feed Title Linkable" description="Enable click link">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>
  <field name="rssdesc" type="extensionradiobutton" default="1" class="radio btn-group btn-group-yesno" label="Feed Description" description="Show the provider's description text for the whole Feed">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>
  <field name="rssimage" type="extensionradiobutton" default="1" class="radio btn-group btn-group-yesno" label="Feed Image" description="Show the provider's image associated with the whole Feed">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>
	<field name="word_count" type="text" size="6" default="0" label="Description Word Count" description="PARAMWORDCOUNT" />	
  <field name="rssitems" type="text" default="5" label="Items" description="Enter number of RSS items to display" />        
  <field name="rssitems_colums" type="text" default="1" label="Items Colums" description="Colums to display" />
  <field name="rssrow_alternate" type="extensionradiobutton" class="radio btn-group btn-group-yesno" default="1" label="Alternate rows" description="Alternate rows format">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>
  <field name="rss_enable_tooltip" type="extensionradiobutton" class="radio btn-group btn-group-yesno" default="1" label="Tooltip Enable" description="TOOLTIP">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>
	<field name="tooltip_wordcount_desc" type="text" size="6" default="0" label="Tooltip Description Word Count" description="TOOLTIPWORDCOUNTDESC" />	
	<field name="rssitemdesc" type="extensionradiobutton" class="radio btn-group btn-group-yesno" default="1" label="Item Description" description="PARAMITEMDESC">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>
  <field name="rssrtl" type="extensionradiobutton" default="0" class="radio btn-group btn-group-yesno" label="RTL feed" description="Display feed in RTL direction">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>
  <field name="link_target" type="extensionradiobutton" default="_blank" label="RSS Feed Link Target" description="Open RSS feed links in this window or a new window?">
			<option value="_blank">New Window</option>
			<option value="_self">Same Window</option>
		</field>
	<field name="no_follow" type="extensionradiobutton" default="0" class="radio btn-group btn-group-yesno" label="Add rel='nofollow' to links" description="Add rel='nofollow' to links">
			<option value="nofollow">JYES</option>
			<option value="0">JNO</option>
		</field>    	
        	
